	<script type="text/javascript">
	function windowResize()
	{
		drawCharts();
	}

	function drawCharts()
	{
					drawEnrollmentByGrade($('#chart-students-by-grade'));

		charts = ['#chart-gender', '#chart-ethnicity', '#chart-lunch-assistance'];

		$.each(charts, function(index, value) {

			var chart = $(value);

			if (chart.length) {
				drawDiffPieChartFromLegend( chart );
			}
		});

		charts = ['#trend-student_teacher_ratio', '#trend-total_students', '#trend-lunch_assistance'];

		$.each(charts, function(index, value) {

			var chart = $(value);

			if (chart.length) {
				var statType = $(value).find('.chart-replacement').data('stattype');
				drawLineChart(chart, statType);
			}
		});
	}

	function drawPieChart(div, chartType)
	{
		var table, title, format, chart;
		var headers = [], values = [];

		table = div.find('table');
		title = table.find('caption').text();
		width = div.width();

		switch(chartType) {

			default:
				format = "###,###";

				generateLabel = function(label, value){
					var label;
					value = formatter.formatValue(value);
					label = '<div style="padding:5px 5px 5px 5px;"><b>' + label + '</b><br/>' + value + '</div>';

					return label;
				}
		}

		var formatter = new google.visualization.NumberFormat(
			{pattern: format});

		// Create the data table.
		var data = new google.visualization.DataTable();
		data.addColumn('string', 'Category');
		data.addColumn('number', 'Value');
		data.addColumn({type: 'string', role: 'style'});
		data.addColumn({type: 'string', role: 'tooltip', p: {'html': true}});

		var oldChartColors = [];

		table.find('tr').each(function(index, value){

			var header = $(this).find('th').text();
			var value = $(this).find('td').text();
			var color = $(this).data('color');

			var label = generateLabel(header, value);

			row = [header, Number(value), color, label];
			data.addRows([row]);

			oldChartColors.push(color.replace('#',''));
		});

		table.hide();

		if( supportsSVG() ){

			var options = {
				title: title,
				titleTextStyle: {fontSize: 16},
				tooltip: { isHtml: true,
					showColorCode: true},
				colors: oldChartColors,

//				legend: 'none',

				legend: {
					alignment: 'center',
					textStyle: {
						fontSize: 14
					}
				},

				pieHole: 0,

				chartArea: {
					left: 0
//					top: 10
//					height: "90%",
//					width: "90%"
				},

				width: width
			};

			formatter.format(data, 1); // Apply formatter to second column

			chart = new google.visualization.PieChart(div.find('.chart').get(0));
		} else {
			var options = {cht: 'p',
				chco: oldChartColors.join("|")
			};

			chart = new google.visualization.ImageChart(div.find('.chart').get(0));
		}

		chart.draw(data, options);
	}

	function drawEnrollmentByGrade(div)
	{
		var table, title, format, chart;
		var headers = [], values = [];

		table = div.find('table');
		title = table.find('caption').text();
		width = div.width();

		format = "###,###";

		generateLabel = function(label, value){
			var label;
			value = formatter.formatValue(value);
			label = '<div style="padding:5px 5px 5px 5px;">Grade: ' + label + '<br/>' + value + '&nbsp;students</div>';

			return label;
		}

		var formatter = new google.visualization.NumberFormat(
			{pattern: format});

		// Create the data table.
		var data = new google.visualization.DataTable();
		data.addColumn('string', 'Category');
		data.addColumn('number', 'Value');
		data.addColumn({type: 'string', role: 'style'});

		data.addColumn({type: 'string', role: 'tooltip', p: {'html': true}});
		data.addColumn({type: 'string', role: 'annotation'});

		var oldChartColors = [];

		table.find('tr').each(function(index, value){

			var header = $(this).find('th').text();
			var value = $(this).find('td').text();
			var color = $(this).data('color');

			var label = generateLabel(header, value);

			row = [header, Number(value), color, label, formatter.formatValue(value)];
			data.addRows([row]);

			oldChartColors.push(color.replace('#',''));
		});

		table.hide();

		if( supportsSVG() ){

			var options = {
				tooltip: { isHtml: true },

				legend: 'none',
				vAxis: {
					format: format,
					textStyle: {fontSize: 12}
				},
				hAxis: {format: format,
					maxAlternation: 1
				},
				chartArea: {
					left:50,
					top:10,
					height:130,
					width: (width - 50)
				},
				height:165,
				width: width
			};

			formatter.format(data, 1); // Apply formatter to second column

			chart = new google.visualization.ColumnChart(div.find('.chart').get(0));
		} else {
			var options = {cht: 'bvs:nda',
				chbh: 'a',
				legend: 'none',
				chco: oldChartColors.join("|")
			};

			chart = new google.visualization.ImageChart(div.find('.chart').get(0));
		}

		chart.draw(data, options);
	}

	function drawDiffPieChartFromLegend(div)
	{
		var tbody, title, chart, legend;
		var oldChartColors = [];

		tbody = div.find('.chart-legend tbody');

		var schoolData = new google.visualization.DataTable();
		schoolData.addColumn('string', 'Category');
		schoolData.addColumn('number', 'Value');
		schoolData.addColumn({type: 'string', role: 'style'});

		var districtData = new google.visualization.DataTable();
		districtData.addColumn('string', 'Category');
		districtData.addColumn('number', 'Value');
		districtData.addColumn({type: 'string', role: 'style'});

		tbody.find('tr').each(function(index, value){

			var label = $(this).find('th span.legend-label').text();
			var color = $(this).find('th').data('color');

			var schoolValue = $(this).find('td.school span.value').text();
			var districtValue = $(this).find('td.district span.value').text();

			oldChartColors.push(color.replace('#',''));

			// Turn strings into numbers
			schoolValue = schoolValue.replace(/[^\d.\- ]/g, '');
			districtValue = districtValue.replace(/[^\d.\- ]/g, '');

			schoolRow = [header, Number(schoolValue), color];
			schoolData.addRows([schoolRow]);

			districtRow = [header, Number(districtValue), color];
			districtData.addRows([districtRow]);
		});

		if( supportsSVG() ){

			var options = {
				title: title,
				titleTextStyle: {fontSize: 16},
				tooltip: {
					isHtml: true,
					showColorCode: true
				},
				colors: oldChartColors,
				legend: 'none',
				pieHole: 0,
				chartArea: {
					left: 0,
					height: "100%",
					width: "100%"
				},
				enableInteractivity: false,
				diff: {
						innerCircle: { radiusFactor: 0.8 },
						newData: {
							inCenter: true,
							opacity: 1
						},
						oldData: {
							inCenter: false,
							opacity: 0.5
						}
					}
			};

			var chartDiff = new google.visualization.PieChart(div.find('.chart-replacement').get(0));
			var diffData = chartDiff.computeDiff(districtData, schoolData);
			chartDiff.draw(diffData, options);
		} else {
			var options = {cht: 'pc',
				chco: oldChartColors.join("|")
			};

			chart = new google.visualization.ImageChart(div.find('.chart-replacement').get(0));
		}
	}

	function drawPieChartFromLegend(div)
	{
		var tbody, title, chart, legend;
		var oldChartColors = [];

		tbody = div.find('.chart-legend tbody');

		var schoolData = new google.visualization.DataTable();
		schoolData.addColumn('string', 'Category');
		schoolData.addColumn('number', 'Value');
		schoolData.addColumn({type: 'string', role: 'style'});

		tbody.find('tr').each(function(index, value){

			var label = $(this).find('th span.legend-label').text();
			var color = $(this).find('th').data('color');

			var schoolValue = $(this).find('td.school span.value').text();

			oldChartColors.push(color.replace('#',''));

			// Turn strings into numbers
			schoolValue = schoolValue.replace(/[^\d.\- ]/g, '');

			schoolRow = [header, Number(schoolValue), color];
			schoolData.addRows([schoolRow]);
		});

		if( supportsSVG() ){

			var options = {
				title: title,
				titleTextStyle: {fontSize: 16},
				tooltip: { isHtml: true,
					showColorCode: true},
				colors: oldChartColors,
				legend: 'none',
				pieHole: 0,
				chartArea: {
					left: 0,
					height: "95%",
					width: "95%"
				}
			};

			chart = new google.visualization.PieChart(div.find('.chart-replacement').get(0));
		} else {
			var options = {cht: 'p',
				chco: oldChartColors.join("|")
			};

			chart = new google.visualization.ImageChart(div.find('.chart-replacement').get(0));
		}

		chart.draw(schoolData, options);
	}

	function drawLineChart(div, statType)
	{
		var label = div.data('label');
		var tbody = div.find('tbody');

		switch(statType) {
			case 'ratio':
				format = "###,###";

				generateLabel = function(label, value){
					var label;
					label = '<div style="padding:5px 5px 5px 5px;"><b>' + label + '</b><br/>' + (Number(value)).toFixed(1) + ':1' + '</div>';

					return label;
				}
				break;

			case 'percentage':
				format = "###,###.##%";

				generateLabel = function(label, value){
					var label;
					label = '<div style="padding:5px 5px 5px 5px;"><b>' + label + '</b><br/>' + (Number(value) * 100).toFixed(2) + '%' + '</div>';

					return label;
				}
				break;

			default:
				format = "###,###";

				generateLabel = function(label, value){
					var label;
					value = formatter.formatValue(value);
					label = '<div style="padding:5px 5px 5px 5px;"><b>' + label + '</b><br/>' + value + '</div>';

					return label;
				}
		}

		var formatter = new google.visualization.NumberFormat(
			{pattern: format});

		var data = new google.visualization.DataTable();
		data.addColumn('string', 'Year');
		data.addColumn('number', label);
		data.addColumn({type: 'string', role: 'tooltip', p: {'html': true}});

		tbody.find('tr').each(function(index, value){

			var header = $(this).find('th').text();
			var year = $(this).find('td:eq(0)').text();
			var itemValue = $(this).find('td:eq(1)').text();

			var label = generateLabel(year, itemValue);

			// Turn strings into numbers
			itemValue = itemValue.replace(/[^\d.\- ]/g, '');

			if(itemValue === '') {
				row = [year, null, label];
			} else {
				row = [year, Number(itemValue), label];
			}

			data.addRows([row]);
		});

		if( supportsSVG() ){

			var options = {
				legend: 'none',
				tooltip: {isHtml: true},
				pointSize: 5,
				vAxis: {format: format}
			};

			chart = new google.visualization.LineChart(div.find('.chart-replacement').get(0));
		} else {
			var options = {cht: 'lc'

			};

			chart = new google.visualization.ImageChart(div.find('.chart-replacement').get(0));
		}

//		var options = {
//			legend: 'none'
//		};
//
//		chart = new google.visualization.LineChart(div.find('.chart-replacement').get(0));
		chart.draw(data, options);


	}
	</script>


	<script>

		$().ready(function(){
			// Site Search in header

			var searchWidth = $("#search_q").width();


			$("#search_q").autocomplete("/autocomplete.php", {
				matchContains: true,
				matchSubset: false,
				extraParams: { type: $("input[name='type']:checked", $('#header_search_form')).val() },
				width: searchWidth
			});
			$("#search_q").result(function(event, data, formatted) {
				if (data) {
					if (data[1] != '') {
						location.href = data[1];
					}
				}
			});
			$("input[name='type']", $('#header_search_form')).click(function() {

				$('#search_q').flushCache();
				$("#search_q").setOptions( {
					extraParams: { type: $("input[name='type']:checked", $('#header_search_form')).val() }
				});
				$('#search_q').onChange();
				$('#search_q').focus();
				$('#search_q').click();
			});

			$("#search_q").bind("keyup", function() {
				$('#search_q_button').val('Loading');
			});

			$("#search_q").bind("focusout", function() {
				$('#search_q_button').val('Search');
			});

			$('.search-form').submit(function(e){
				e.preventDefault();
			});


			// School Search in Right Column
			var sideWidth = $("#school").width();

			var selected_value = '';
			$("#school").autocomplete("/autoSchools.php", {
				matchContains: true,
				matchSubset: false,
				//mustMatch: false,
				width: sideWidth
			});
			$("#school").result(function(event, data, formatted) {
				if (data) {
					$("#school_id").val(data[1]);
					selected_value = $("#school").val();
				}
			});
			$('#school').keyup(function() {
				if (selected_value != $("#school").val()) {
					$('#school_id').val('');
				}
			});


			$("#school").bind("keyup", function() {
				$('#school_button').val('Loading');
			});

			$("#school").bind("focusout", function() {
				$('#school_button').val('Search Alumni');
			});



			// School Search in Footer
			var sideWidth = $("#footer-search-input").width();

			var selected_value = '';
			$("#footer-search-input").autocomplete("/autoSchools.php", {
				matchContains: true,
				matchSubset: false,
				width: sideWidth
			});
			$("#footer-search-input").result(function(event, data, formatted) {
				if (data) {
					$("#school_id").val(data[1]);
					selected_value = $("#school").val();
				}
			});
			$('#footer-search-input').keyup(function() {
				if (selected_value != $("#footer-search-input").val()) {
					$('#school_id').val('');
				}
			});


			$("#footer-search-input").bind("keyup", function() {
				$('#footer-search-btn').val('Loading');
			});

			$("#footer-search-input").bind("focusout", function() {
				$('#footer-search-btn').val('Search Alumni');
			});
		});
	</script>

